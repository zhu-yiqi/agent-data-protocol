{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "website": {
      "type": "string"
    },
    "domain": {
      "type": "string"
    },
    "confirmed_task": {
      "type": "string"
    },
    "annotation_id": {
      "type": "string"
    },
    "action_reprs": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "actions": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "action_uid": {
            "type": "string"
          },
          "cleaned_html": {
            "type": "string"
          },
          "neg_candidates": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "attributes": {
                  "type": "string"
                },
                "backend_node_id": {
                  "type": "string"
                },
                "tag": {
                  "type": "string"
                }
              },
              "required": ["attributes", "backend_node_id", "tag"]
            }
          },
          "operation": {
            "type": "object",
            "properties": {
              "op": {
                "type": "string"
              },
              "original_op": {
                "type": "string"
              },
              "value": {
                "type": "string"
              }
            },
            "required": ["op", "original_op", "value"]
          },
          "pos_candidates": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "attributes": {
                  "type": "string"
                },
                "backend_node_id": {
                  "type": "string"
                },
                "is_original_target": {
                  "type": "boolean"
                },
                "is_top_level_target": {
                  "type": "boolean"
                },
                "tag": {
                  "type": "string"
                }
              },
              "required": ["attributes", "backend_node_id", "is_original_target", "is_top_level_target", "tag"]
            }
          },
          "raw_html": {
            "type": "string"
          }
        },
        "required": ["action_uid", "cleaned_html", "neg_candidates", "operation", "pos_candidates", "raw_html"]
      }
    },
    "subdomain": {
      "type": "string"
    }
  },
  "required": ["website", "domain", "confirmed_task", "annotation_id", "action_reprs", "actions", "subdomain"]
}