{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "image/width": { "type": "integer" },
    "image/height": { "type": "integer" },
    "results/yx_touch": {
      "type": "array",
      "items": { "type": "number" },
      "minItems": 2,
      "maxItems": 2
    },
    "results/type_action": { "type": "string" },
    "android_api_level": { "type": "integer" },
    "image/ui_annotations_text": {
      "type": "array",
      "items": { "type": "string" }
    },
    "results/yx_lift": {
      "type": "array",
      "items": { "type": "number" },
      "minItems": 2,
      "maxItems": 2
    },
    "image/ui_annotations_ui_types": {
      "type": "array",
      "items": { "type": "string" }
    },
    "current_activity": { "type": "string" },
    "step_id": { "type": "integer" },
    "image/channels": { "type": "integer" },
    "episode_length": { "type": "integer" },
    "image/encoded": { "type": "string" },
    "results/action_type": { "type": "string" },
    "goal_info": { "type": "string" },
    "image/ui_annotations_positions": {
      "type": "array",
      "items": {
        "type": "array",
        "items": { "type": "number" },
        "minItems": 4,
        "maxItems": 4
      }
    },
    "episode_id": { "type": "string" },
    "device_type": { "type": "string" }
  },
  "required": [
    "image/width",
    "image/height",
    "results/yx_touch",
    "results/type_action",
    "android_api_level",
    "image/ui_annotations_text",
    "results/yx_lift",
    "image/ui_annotations_ui_types",
    "current_activity",
    "step_id",
    "image/channels",
    "episode_length",
    "image/encoded",
    "results/action_type",
    "goal_info",
    "image/ui_annotations_positions",
    "episode_id",
    "device_type"
  ]
}